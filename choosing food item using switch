#include <stdio.h>
    int main() {
        int num;
        scanf("%d", &num);
        switch (num) {
            case 1:
                printf("Food item- Biryani\n");
                printf("Price- Rs 200");


                break;
            case 2:
                printf("Food item- fried rice\n");
                printf("Price- Rs 250");

                break;
            case 3:
               printf("Food item- manchurian\n");
                printf("Price- Rs 80");

                break;
            case 4:
               printf("Food item- soup\n");
                printf("Price- Rs 100");

                break;
             case 5:
               printf("Food item- noodles");
                printf("Price- Rs 150");

                break;
        }
        return 0;
    }
