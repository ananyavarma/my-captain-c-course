#include <stdio.h>
int main()
{
    int x=0,y=0;
    printf("Enter value of x:");
    scanf("%d", &x);
        printf("Enter value of y:");
    scanf("%d", &y);
    x = x + y; // x now becomes 15
    y = x - y; // y becomes 10
    x = x - y; // x becomes 5

    printf("After Swapping the values are: x = %d, y = %d", x, y);

    return 0;
}
